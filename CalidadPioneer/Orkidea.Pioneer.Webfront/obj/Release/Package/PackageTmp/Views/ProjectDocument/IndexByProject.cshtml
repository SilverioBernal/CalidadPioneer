@model IEnumerable<Orkidea.Pioneer.Entities.ProjectDocument>
@{
    Layout = null;
}
<ul>
    @foreach (var item in Model)
    {
        <li class="itemdoc">
            <span><i class="glyphicon glyphicon-paperclip"></i> <a href="@Url.Action("Details", new {archivo = item.nombre })" target=" _blank">@item.descripcion</a></span>
            <a href="javascript:void(0)" id="@item.id" class="linkDoc1">Ver relacionados</a>
        </li>
    }
</ul>